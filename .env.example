# Get these from https://developer.withings.com/dashboard
WITHINGS_CLIENT_ID=your_client_id
WITHINGS_CLIENT_SECRET=your_client_secret

# Set this to your Vercel URL + /api/callback after deploying
WITHINGS_REDIRECT_URI=https://your-app.vercel.app/api/callback

# These are obtained after OAuth authorization (leave empty initially)
WITHINGS_ACCESS_TOKEN=
WITHINGS_REFRESH_TOKEN=
